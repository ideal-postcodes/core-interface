{
  "name": "@ideal-postcodes/core-interface",
  "version": "1.5.0",
  "description": "Interface specification for javascript based API Clients to api.ideal-postcodes.co.uk",
  "author": {
    "name": "Ideal Postcodes",
    "email": "support@ideal-postcodes.co.uk",
    "url": "https://ideal-postcodes.co.uk"
  },
  "bugs": {
    "url": "https://github.com/ideal-postcodes/core-interface/issues",
    "email": "support@ideal-postcodes.co.uk"
  },
  "homepage": "https://github.com/ideal-postcodes/core-interface",
  "repository": {
    "type": "git",
    "url": "https://github.com/ideal-postcodes/core-interface.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "semantic-release": "semantic-release --no-ci",
    "test-watch": "NODE_ENV=test mocha --watch",
    "test": "npm run lint && NODE_ENV=test nyc mocha",
    "test:browsers:headless": "karma start",
    "test:browsers": "npm run test:browsers:latest && npm run test:browsers:legacy",
    "test:browsers:latest": "karma start .config/latest.conf.ts",
    "test:browsers:legacy": "karma start .config/legacy.conf.ts",
    "lint": "eslint lib/**/*.ts",
    "build": "npm run lint && tsc",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "typedoc --mode file --out docs/ --name 'Ideal Postcodes JS Client Interface' --theme minimal --excludePrivate --hideGenerator lib/",
    "prepublishOnly": "npm run test && npm run build"
  },
  "files": [
    "dist"
  ],
  "prettier": {
    "trailingComma": "es5"
  },
  "nyc": {
    "include": [
      "lib/**/*.ts",
      "generate/**/*.ts"
    ],
    "exclude": [
      "dist/*",
      "**/*.d.ts",
      "generate/index.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "engines": {
    "node": ">=8.0"
  },
  "keywords": [
    "API",
    "Interface",
    "UK",
    "Address",
    "Postcode",
    "Search",
    "Ideal Postcodes"
  ],
  "release": {
    "extends": "@cablanchard/semantic-release/dist/npm"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@cablanchard/eslint-config": "~1.1.1",
    "@cablanchard/semantic-release": "~1.3.1",
    "@cablanchard/tsconfig": "1.0.0",
    "@cablanchard/tslint": "0.0.1",
    "@ideal-postcodes/api-fixtures": "~1.1.0",
    "@ideal-postcodes/api-typings": "~1.1.0",
    "@ideal-postcodes/supported-browsers": "~2.3.2",
    "@types/chai": "~4.2.0",
    "@types/dotenv": "~8.2.0",
    "@types/karma": "~5.0.0",
    "@types/mocha": "~7.0.1",
    "@types/node": "~14.0.5",
    "@types/prettier": "~2.0.0",
    "@types/puppeteer": "~3.0.0",
    "@types/sinon": "~9.0.1",
    "chai": "~4.2.0",
    "codecov": "~3.7.0",
    "core-js": "~3.6.4",
    "dotenv": "~8.2.0",
    "eslint": "~6.8.0",
    "karma": "~5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-mocha": "~2.0.0",
    "karma-polyfill": "~1.1.0",
    "karma-sauce-launcher": "~1.2.0",
    "karma-typescript": "~5.0.0",
    "mocha": "~7.2.0",
    "nyc": "~15.0.0",
    "prettier": "~2.0.2",
    "puppeteer": "~3.2.0",
    "regenerator-runtime": "~0.13.3",
    "semantic-release": "~17.0.7",
    "sinon": "~9.0.0",
    "source-map-support": "~0.5.10",
    "ts-node": "~8.10.1",
    "tslint": "~5.20.0",
    "type-zoo": "~3.4.0",
    "typedoc": "~0.17.1",
    "typescript": "~3.9.3"
  }
}
