{
  "name": "@ideal-postcodes/core-interface",
  "version": "3.2.1",
  "description": "Interface specification for javascript based API Clients to api.ideal-postcodes.co.uk",
  "author": {
    "name": "Ideal Postcodes",
    "email": "support@ideal-postcodes.co.uk",
    "url": "https://ideal-postcodes.co.uk"
  },
  "bugs": {
    "url": "https://github.com/ideal-postcodes/core-interface/issues",
    "email": "support@ideal-postcodes.co.uk"
  },
  "homepage": "https://github.com/ideal-postcodes/core-interface",
  "repository": {
    "type": "git",
    "url": "https://github.com/ideal-postcodes/core-interface.git"
  },
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "semantic-release": "semantic-release --no-ci",
    "test-watch": "NODE_ENV=test mocha --watch",
    "test": "NODE_ENV=test nyc mocha",
    "test:browsers:headless": "karma start",
    "test:browsers": "npm run test:browsers:latest && npm run test:browsers:legacy",
    "test:browsers:latest": "karma start .config/latest.conf.ts",
    "test:browsers:legacy": "karma start .config/legacy.conf.ts",
    "lint": "eslint lib/**/*.ts",
    "build": "tsc && tsc -p tsconfig.esm.json",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "typedoc",
    "prepublishOnly": "npm run test && npm run build"
  },
  "files": [
    "dist",
    "esm"
  ],
  "browserslist": [
    "ie 11",
    "last 2 versions"
  ],
  "prettier": {
    "trailingComma": "es5"
  },
  "nyc": {
    "include": [
      "lib/**/*.ts",
      "generate/**/*.ts"
    ],
    "exclude": [
      "dist/*",
      "**/*.d.ts",
      "generate/index.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "engines": {
    "node": ">=8.0"
  },
  "keywords": [
    "API",
    "Interface",
    "UK",
    "Address",
    "Postcode",
    "Search",
    "Ideal Postcodes"
  ],
  "release": {
    "extends": "@cablanchard/semantic-release/dist/npm",
    "branches": [
      "master",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@ideal-postcodes/openapi": "3.1.0"
  },
  "devDependencies": {
    "@babel/polyfill": "~7.12.1",
    "@cablanchard/eslint-config": "~3.1.1",
    "@cablanchard/semantic-release": "~1.3.1",
    "@cablanchard/tsconfig": "2.0.0",
    "@cablanchard/tslint": "0.0.1",
    "@ideal-postcodes/api-fixtures": "~1.3.0",
    "@ideal-postcodes/doc-assets": "~1.0.3",
    "@ideal-postcodes/supported-browsers": "~2.5.0",
    "@types/chai": "~4.3.0",
    "@types/dotenv": "~8.2.0",
    "@types/karma": "~6.3.0",
    "@types/mocha": "~9.0.0",
    "@types/node": "~18.7.16",
    "@types/prettier": "~2.6.1",
    "@types/sinon": "~10.0.0",
    "@typescript-eslint/eslint-plugin": "~5.8.0",
    "chai": "~4.3.0",
    "codecov": "~3.8.0",
    "core-js": "~3.25.0",
    "dotenv": "~8.2.0",
    "eslint": "~7.32.0",
    "eslint-plugin-compat": "~3.13.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-mocha": "~2.0.0",
    "karma-polyfill": "~1.1.0",
    "karma-sauce-launcher": "~4.3.3",
    "karma-typescript": "~5.5.1",
    "karma-typescript-es6-transform": "~5.5.2",
    "mocha": "~9.1.0",
    "nyc": "~15.1.0",
    "prettier": "~2.6.2",
    "puppeteer": "~13.0.1",
    "regenerator-runtime": "~0.13.3",
    "semantic-release": "~17.4.1",
    "sinon": "~12.0.1",
    "source-map-support": "~0.5.10",
    "ts-node": "~10.4.0",
    "tslint": "~6.1.2",
    "type-zoo": "~3.4.0",
    "typedoc": "^0.21.0",
    "typescript": "~4.4.3"
  }
}
